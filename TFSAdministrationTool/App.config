<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="TFSAdministrationTool.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <system.diagnostics>
    <switches>
      <add name="TFSAdminToolTraceSwitch" value="4" />
    </switches>
  </system.diagnostics>
  <system.net>
    <!-- Please fill in these settings in order to be able to send mail notifications-->
    <mailSettings>
      <smtp from="entrer une adresse email ici">
        <network host="smtp.aql.fr" port="25" userName="" password="" defaultCredentials="true" />
      </smtp>
    </mailSettings>
  </system.net>
  <userSettings>
    <TFSAdministrationTool.Properties.Settings>
      <setting name="PromptForSystemAvaiablity" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="NotifyUsersByEmail" serializeAs="String">
        <value>True</value>
      </setting>
    </TFSAdministrationTool.Properties.Settings>
  </userSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.TeamFoundation.Client" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
